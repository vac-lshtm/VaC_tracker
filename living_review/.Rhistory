fill = Plotgroup, label = Responseplot)) +
geom_bar(stat="identity") + geom_text(nudge_y = 8, aes(colour = Plotgroup), size=4) +
theme_bw() +
scale_fill_manual(values = palette[1:ngroup]) + scale_colour_manual(values = palette[1:ngroup]) +
ylab("%") + xlab("") +
